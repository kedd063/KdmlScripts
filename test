local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local player = Players.LocalPlayer

-- GUI
local gui = Instance.new("ScreenGui")
gui.Name = "TpInsideGui"
gui.Parent = player:WaitForChild("PlayerGui")
gui.ResetOnSpawn = false
gui.IgnoreGuiInset = true

-- Buton
local button = Instance.new("TextButton")
button.Size = UDim2.new(0, 160, 0, 50)
button.Position = UDim2.new(0, 20, 0.85, 0)
button.BackgroundColor3 = Color3.fromRGB(140, 90, 255)
button.TextColor3 = Color3.new(1, 1, 1)
button.Font = Enum.Font.GothamBold
button.TextSize = 20
button.Text = "TP Inside"
button.Name = "TpInsideButton"
button.Parent = gui

local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 12)
corner.Parent = button

-- Fonksiyon
local function wallTP()
    local character = player.Character or player.CharacterAdded:Wait()
    local hrp = character:FindFirstChild("HumanoidRootPart")
    if not hrp then return end

    -- NoClip Aç
    for _, part in ipairs(character:GetDescendants()) do
        if part:IsA("BasePart") then
            part.CanCollide = false
        end
    end

    -- Hedef nokta
    local look = workspace.CurrentCamera.CFrame.LookVector
    local newPos = hrp.Position + look * 5
    newPos = Vector3.new(newPos.X, hrp.Position.Y, newPos.Z)

    -- Tween ile geç
    local tween = TweenService:Create(hrp, TweenInfo.new(0.3), {CFrame = CFrame.new(newPos)})
    tween:Play()

    -- Collision geri aç
    tween.Completed:Connect(function()
        for _, part in ipairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = true
            end
        end
    end)
end

-- Butona basınca çalıştır
button.MouseButton1Click:Connect(wallTP)
