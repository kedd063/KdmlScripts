local raw = game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua")
local Material = loadstring(raw)()

local UI = Material.Load({
    Title = "KDML Scripts",
    Style = 1,
    SizeX = 400,
    SizeY = 300,
    Theme = "Dark"
})

-- Sayfalar
local MainPage = UI.New({ Title = "Main" })
local VisualPage = UI.New({ Title = "Visual" })
local MiscPage = UI.New({ Title = "Misc" })
local OtherPage = UI.New({ Title = "Other" })

-- TEST MODU üéÆ: Tu≈ülar g√∂r√ºn√ºr ama sadece print yapar

-- Main B√∂l√ºm√º
MainPage.Button({
    Text = "Free Cam",
    Callback = function() print("[TEST] Free Cam") end
})
MainPage.Button({
    Text = "Speed Boost",
    Callback = function() print("[TEST] Speed Boost") end
})
MainPage.Button({
    Text = "Jump Boost",
    Callback = function() print("[TEST] Jump Boost") end
})
MainPage.Button({
    Text = "Float",
    Callback = function() print("[TEST] Float") end
})
MainPage.Button({
    Text = "TP To Coords",
    Callback = function() print("[TEST] TP 1") end
})
MainPage.Button({
    Text = "TP To Coords V2",
    Callback = function() print("[TEST] TP 2") end
})

-- Visual B√∂l√ºm√º
VisualPage.Toggle({
    Text = "ESP",
    Callback = function(state) print("[TEST] ESP State:", state) end
})
VisualPage.Toggle({
    Text = "Secret Brainrots ESP",
    Callback = function(state) print("[TEST] Secret ESP:", state) end
})
VisualPage.Toggle({
    Text = "God Brainrots ESP",
    Callback = function(state) print("[TEST] God ESP:", state) end
})

-- Misc B√∂l√ºm√º
local items = { "Trap", "Invisibility Cloak", "Medusa's Head", "Quantum Cloner", "Body Swap Potion" }
for _, item in pairs(items) do
    MiscPage.Button({
        Text = item,
        Callback = function() print("[TEST] Item:", item) end
    })
end

-- Other B√∂l√ºm√º
OtherPage.Toggle({
    Text = "Auto Candy Spin",
    Callback = function(state) print("[TEST] Candy Spin:", state) end
})
OtherPage.Button({
    Text = "Server Hop",
    Callback = function() print("[TEST] Server Hop") end
})
OtherPage.Toggle({
    Text = "FPS Boost",
    Callback = function(state) print("[TEST] FPS Boost:", state) end
})

-- UI'yi ba≈ülat
UI:Init()
