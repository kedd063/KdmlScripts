local VenyxLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/yenblox/Venyx-UI-Library/main/source.lua"))()
local venyx = VenyxLib.new("KDML SCRIPTS - TEST MODE", 5013109572)

local main = venyx:addPage("Main", 5012544693)
local visual = venyx:addPage("Visual", 5012544693)
local misc = venyx:addPage("Misc", 5012544693)
local other = venyx:addPage("Other", 5012544693)

local secMain = main:addSection("Main Features")
local secVisual = visual:addSection("ESP Options")
local secMisc = misc:addSection("Items")
local secOther = other:addSection("Utilities")

-- Her şeyde sadece print fonksiyonu
local function noAction()
    print("[TEST MODE] Butona tıklandı, ama çalışmaz :)")
end

-- Main sekmesi
secMain:addButton("Free Cam", noAction)
secMain:addButton("Speed Boost", noAction)
secMain:addButton("Jump Boost", noAction)
secMain:addButton("Float", noAction)
secMain:addButton("TP To Coords", noAction)
secMain:addButton("TP To Coords V2", noAction)

-- Visual sekmesi
secVisual:addToggle("ESP", false, function(val)
    print("[TEST MODE] ESP toggle: ", val)
end)
secVisual:addToggle("Secret Brainrots ESP", false, function(val)
    print("[TEST MODE] Secret Brainrots toggle: ", val)
end)
secVisual:addToggle("God Brainrots ESP", false, function(val)
    print("[TEST MODE] God Brainrots toggle: ", val)
end)

-- Misc sekmesi
local items = {"Trap", "Invisibility Cloak", "Medusa's Head", "Quantum Cloner", "Body Swap Potion"}
for _, item in pairs(items) do
    secMisc:addButton(item, function()
        print("[TEST MODE] Item button pressed: " .. item)
    end)
end

-- Other sekmesi
secOther:addToggle("Auto Candy Spin", false, function(val)
    print("[TEST MODE] Candy Spin toggle: ", val)
end)

secOther:addButton("Server Hop", noAction)
secOther:addToggle("FPS Boost", false, function(val)
    print("[TEST MODE] FPS Boost toggle: ", val)
end)

-- UI aç
venyx:SelectPage(venyx.pages[1], true)
